{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "airbnb-base",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended"
  ],
  "rules":{
      "quotes":["error","single"],
      "no-console": 1,
      "complexity": ["error", 3],
      "class-methods-use-this": [0]
  }
}